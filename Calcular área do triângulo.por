programa {
  funcao inicio() {
    real base, altura, area
    escreva ("Qual é a base do seu triângulo? ")
    leia (base)
    escreva ("Qual é a altura do seu triângulo? ")
    leia (altura)
    area = base*altura/2
    escreva ("Seu triângulo tem: \nBase: ", base, "\nAltura: ", altura, "\nÁrea: ", area )
  }
}
